{
    "name": "Nyika",
    "id":"extern.web.app",
    "description": "Explore the Web!",
    "version": "0.1",
    "author": "Anesu",
    "main": "index.html",
    "multi_window" : true,
    "category" : "Internet",
    "window": {
        "height": 711,
        "width": 1294,
        "show": true,
        "title": "Novaweb",
        "transparent": true,
        "frame": false,
        "icon": "abrowser2.svg"
    },
    "webview": {
     "partitions": [
        {
          "name": "trusted",
          "accessible_resources": [ "<all_urls>" ]
        }
     ]
  },
    "chromium-args": "--enable-transparent-visuals"
}
