prefix=/usr
libdir=${prefix}/lib/x86_64-linux-gnu
includedir=${prefix}/include

glx_tls=yes

Name: gl
Description: Mesa OpenGL Library
Version: 19.2.1
Requires.private: xxf86vm x11-xcb xext xdamage >= 1.1 x11 xcb-dri2 >= 1.8 xcb xcb-glx >= 1.8.1 xfixes libdrm >= 2.4.75
Libs: -L${libdir} -lGL
Libs.private: -pthread -ldl -lpthread -lm
Cflags: -I${includedir}
